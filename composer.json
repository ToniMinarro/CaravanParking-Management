{
  "name": "toni/caravan-parking-management",
  "license": "MIT",
  "type": "project",
  "description": "Simple application to manage IN/OUT in a caravans parking area ",
  "require": {
    "php": "^8.2",

    "ext-apcu": "*",
    "ext-json": "*",
    "ext-zend-opcache": "*",
    "ext-pdo": "*",

    "symfony/framework-bundle": "^6",
    "symfony/dotenv": "^6",
    "symfony/yaml": "^6",
    "symfony/twig-bundle": "^6",
    "symfony/validator": "^6",
    "symfony/cache": "^6",

    "lambdish/phunctional": "^2",

    "ramsey/uuid": "^4",

    "doctrine/dbal": "^3",
    "doctrine/orm": "^2",

    "ocramius/proxy-manager": "^2",
    "laminas/laminas-zendframework-bridge": "^1",


    "endclothing/prometheus_client_php": "^1"
  },
  "require-dev": {
    "ext-xdebug": "*",

    "roave/security-advisories": "dev-master",

    "friends-of-behat/mink-extension": "^2",
    "friends-of-behat/symfony-extension": "^2",
    "behat/mink-browserkit-driver": "^2",

    "phpunit/phpunit": "^9",
    "mockery/mockery": "^1",

    "fakerphp/faker": "^1",

    "symfony/error-handler": "^6",
    "vimeo/psalm": "^4",
    "friendsofphp/php-cs-fixer": "^3.8"
  },
  "autoload": {
    "psr-4": {
      "Toni\\Apps\\CaravanManagement\\Backend\\": "apps/caravanManagement/backend/src",
      "Toni\\Apps\\CaravanManagement\\Frontend\\": "apps/caravanManagement/frontend/src",

      "Toni\\": ["src"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Toni\\Tests\\": ["tests"]
    }
  },
  "minimum-stability": "RC",
  "config": {
    "allow-plugins": {
      "ocramius/package-versions": true
    }
  }
}
